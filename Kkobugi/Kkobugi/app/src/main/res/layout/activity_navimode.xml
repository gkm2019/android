<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.NavimodeActivity"
    android:background="@color/colorWhite">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_marginTop="25dp"
        android:padding="70dp"
        android:gravity="center">


        <org.ros.android.view.RosTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/rostextview_complete"
            android:text="test"
            android:textSize="30dp"
            android:visibility="invisible"/>

        <TextView
            android:id="@+id/text_navimode"
            android:layout_width="wrap_content"
            android:layout_height="130dp"
            android:text=""
            android:fontFamily="@font/joseonilbomyeongjo"
            android:textSize="20dp"
            android:lineSpacingExtra="10dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>

        <ViewFlipper
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/image_slide"
            android:src="@drawable/ic_mic"
            android:background="@color/colorWhite"
            android:scaleType="fitCenter"/>

        <ProgressBar
            android:id="@+id/progressBar_hori"
            style="?android:attr/progressBarStyleHorizontal"
            android:progressDrawable="@drawable/progressbar_hori"
            android:layout_marginTop="15dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

    </LinearLayout>
</LinearLayout>
